<?xml version="1.0" encoding="utf-8"?>
<odoo>
  <data noupdate="1">
    <!-- Ítem de menú en el Website que apunta a /calculator -->
    <record id="website_menu_calculator" model="website.menu">
      <field name="name">Calculadora</field>
      <field name="url">/calculator</field>
      <field name="parent_id" ref="website.main_menu"/>
      <field name="sequence" eval="95"/>
      <!-- Opcional en multi-website:
      <field name="website_id" ref="website.default_website"/>
      -->
    </record>
  </data>
</odoo>
